package com.company;


public class Main {

    public static void main(String[] args) {
        Circle cirko = new Circle();
        cirko.makeVisible();
        String caler[] = {"red", "black", "blue", "yellow", "green", "magenta", "white", "啊这"};
        for (int i = 0; i < 100; i++) {
            cirko.changeColor(caler[i % 8]);
        }
    }
}

